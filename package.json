{
  "name": "kysely-params",
  "version": "0.3.8",
  "author": "Joseph T. Lapp <arachnojoe@gmail.com>",
  "license": "MIT",
  "description": "A utility for parameterizing compiled Kysely queries",
  "keywords": [
    "kysely",
    "compiled",
    "cached",
    "parameterized",
    "parameters"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "rm -rdf dist && tsc && tsc -p tsconfig.cjs.json",
    "test": "jest"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.5",
    "better-sqlite3": "^8.2.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "kysely": "^0.24.2"
  }
}
